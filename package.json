{
  "name": "lightstep-opentelemetry-launcher-node",
  "version": "1.0.0",
  "description": "Lightstep Launcher - a configuration layer OpenTelemetry Node.js",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "lightstep/otel-launcher-node",
  "scripts": {
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ./",
    "example": "node example/index.js",
    "build": "npm run compile",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "precompile": "tsc --version",
    "release": "./scripts/release.sh",
    "release:prepare": "./scripts/release-prepare.sh",
    "version:update": "node ./scripts/version-update.js",
    "compile": "npm run version:update && tsc -p .",
    "prepare": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json test/**/*.test.ts",
    "watch": "tsc -w"
  },
  "keywords": [
    "lightstep",
    "opentelemetry",
    "nodejs",
    "tracing",
    "profiling",
    "metrics",
    "stats",
    "monitoring"
  ],
  "author": "Lightstep",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/api-metrics": "^0.26.0",
    "@opentelemetry/auto-instrumentations-node": "~0.26.0",
    "@opentelemetry/core": "~1.0.0",
    "@opentelemetry/exporter-otlp-http": "~0.26.0",
    "@opentelemetry/instrumentation": "~0.26.0",
    "@opentelemetry/propagator-b3": "~1.0.0",
    "@opentelemetry/shim-opentracing": "~1.0.0",
    "@opentelemetry/resources": "~1.0.0",
    "@opentelemetry/sdk-trace-base": "~1.0.0",
    "@opentelemetry/sdk-trace-node": "~1.0.0",
    "@opentelemetry/semantic-conventions": "~1.0.0",
    "@opentelemetry/resource-detector-aws": "~1.0.0",
    "@opentelemetry/resource-detector-gcp": "~0.26.0",
    "@opentelemetry/sdk-metrics-base": "~0.26.0",
    "opentracing": "^0.14.5",
    "@types/express": "4.17.11",
    "@types/graphql": "14.5.0",
    "@types/hapi__hapi": "20.0.8",
    "@types/ioredis": "4.26.0",
    "@types/koa": "2.13.1",
    "@types/koa__router": "8.0.4",
    "@types/mongodb": "3.6.12",
    "@types/mysql": "2.15.18",
    "@types/pg": "7.14.11",
    "@types/pg-pool": "2.0.2",
    "@types/redis": "2.8.28"
  },
  "devDependencies": {
    "@types/mocha": "8.0.3",
    "@types/node": "14.14.6",
    "@types/sinon": "^9.0.8",
    "codecov": "3.8.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.3",
    "gts": "3.0.2",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "package-json": "^6.5.0",
    "sinon": "^9.2.1",
    "ts-mocha": "8.0.0",
    "typescript": "^4.2.4"
  }
}
